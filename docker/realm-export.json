{
  "realm": "nemakiware",
  "enabled": true,
  "clients": [
    {
      "clientId": "nemakiware-oidc-client",
      "protocol": "openid-connect",
      "publicClient": true,
      "standardFlowEnabled": true,
      "redirectUris": [
        "http://localhost:8080/core/ui/*",
        "http://localhost:8080/core/ui/silent-callback.html"
      ],
      "webOrigins": [
        "http://localhost:8080"
      ]
    },
    {
      "clientId": "nemakiware-saml-client",
      "protocol": "saml",
      "adminUrl": "http://localhost:8080/core/ui/saml-callback.html",
      "redirectUris": [
        "http://localhost:8080/core/ui/saml-callback.html"
      ],
      "attributes": {
        "saml.force.post.binding": "true"
      }
    }
  ],
  "users": [
    {
      "username": "testuser",
      "enabled": true,
      "credentials": [
        {
          "type": "password",
          "value": "password",
          "temporary": false
        }
      ]
    }
  ]
}
