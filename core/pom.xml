										<include name="chemistry-opencmis-commons-api-1.1.0.jar"/>
										<include name="chemistry-opencmis-server-bindings-1.1.0.jar"/>
										<include name="chemistry-opencmis-server-support-1.1.0.jar"/>
										<include name="chemistry-opencmis-client-bindings-1.1.0.jar"/>
										<include name="chemistry-opencmis-client-api-1.1.0.jar"/>
										<include name="chemistry-opencmis-client-impl-1.1.0.jar"/>
										<!-- System-scoped Jakarta JARs with -jakarta suffix - ALL MUST BE REMOVED -->
										<include name="chemistry-opencmis-commons-impl-1.1.0-jakarta.jar"/>
										<include name="chemistry-opencmis-commons-api-1.1.0-jakarta.jar"/>
										<include name="chemistry-opencmis-client-bindings-1.1.0-jakarta.jar"/>
										<include name="chemistry-opencmis-server-bindings-1.1.0-jakarta.jar"/>
										<include name="chemistry-opencmis-server-support-1.1.0-jakarta.jar"/>
										<include name="chemistry-opencmis-client-api-1.2.0-nemakiware-jakarta.jar"/>
										<include name="chemistry-opencmis-client-impl-1.2.0-nemakiware-jakarta.jar"/>
									</fileset>
								</delete>

								<!-- UNIFIED JAR MANAGEMENT: Copy 1.1.0-nemakiware Self-Build JARs from built-jars -->
								<!-- Jakarta EE 10 requires self-build OpenCMIS 1.1.0-nemakiware -->
								<copy todir="${project.build.directory}/${project.build.finalName}/WEB-INF/lib">
									<fileset dir="${basedir}/../lib/built-jars">
										<include name="chemistry-opencmis-server-bindings-1.1.0-nemakiware.jar"/>
										<include name="chemistry-opencmis-commons-impl-1.1.0-nemakiware.jar"/>
										<include name="chemistry-opencmis-commons-api-1.1.0-nemakiware.jar"/>
										<include name="chemistry-opencmis-server-support-1.1.0-nemakiware.jar"/>
										<include name="chemistry-opencmis-client-impl-1.1.0-nemakiware.jar"/>
										<include name="chemistry-opencmis-client-api-1.1.0-nemakiware.jar"/>
										<include name="chemistry-opencmis-client-bindings-1.1.0-nemakiware.jar"/>
										<include name="chemistry-opencmis-test-tck-1.1.0-nemakiware.jar"/>
									</fileset>
								</copy>
								<echo message="Copied Jakarta EE 10 compatible OpenCMIS 1.1.0-nemakiware JARs from fixed MultipartParser locations"/>
								<!-- jaxws-rt-4.0.2-jakarta.jar copy removed - files moved to suspended-modules -->
							</target>
						</configuration>
					</execution>
				</executions>
			</plugin>
		</plugins>
	</build>

