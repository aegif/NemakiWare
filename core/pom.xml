					<execution>
						<id>run-jetty-tck</id>
						<goals>
							<goal>java</goal>
						</goals>
						<configuration>
							<mainClass>jp.aegif.nemaki.cmis.tck.JettyTckRunner</mainClass>
						</configuration>
