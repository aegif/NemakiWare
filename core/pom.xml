					<scan>1</scan>
					<stopPort>9967</stopPort>
					<stopKey>foo</stopKey>
					<!-- Skip Jetty during test phase to avoid port conflicts -->
					<skip>true</skip>
					<webApp>
						<contextPath>/core</contextPath>
						<!-- Priority: Jakarta JARs FIRST to override standard OpenCMIS classes -->
						<extraClasspath>${project.basedir}/../lib/built-jars/chemistry-opencmis-server-bindings-1.1.0-nemakiware.jar,${project.basedir}/../lib/built-jars/chemistry-opencmis-commons-impl-1.1.0-nemakiware.jar,${project.basedir}/../lib/built-jars/chemistry-opencmis-commons-api-1.1.0-nemakiware.jar,${project.basedir}/../lib/built-jars/chemistry-opencmis-server-support-1.1.0-nemakiware.jar</extraClasspath>
