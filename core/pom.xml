							<goal>java</goal>
						</goals>
						<configuration>
							<mainClass>jp.aegif.nemaki.cmis.tck.QueryValidationRunner</mainClass>
						</configuration>
					</execution>
				</executions>
			</plugin>

			<!-- Antrun plugin to copy Jakarta converted JARs -->
			<plugin>
				<artifactId>maven-antrun-plugin</artifactId>
				<version>3.0.0</version>
				<executions>
