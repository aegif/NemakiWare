							<value>http://localhost:5984</value>
						</systemProperty>
						<systemProperty>
							<name>spring.profiles.active</name>
							<value>development</value>
						</systemProperty>
						<systemProperty>
							<name>org.ektorp.support.AutoUpdateViewOnChange</name>
							<value>false</value>
						</systemProperty>
						<systemProperty>
							<name>org.ektorp.http.IdleConnectionMonitor.enabled</name>
							<value>false</value>
						</systemProperty>
						<systemProperty>
							<name>org.ektorp.support.DesignDocument.UPDATE_ON_DIFF</name>
							<value>false</value>
						</systemProperty>
						<systemProperty>
							<name>org.ektorp.support.DesignDocument.ALLOW_AUTO_UPDATE</name>
							<value>true</value>
						</systemProperty>
