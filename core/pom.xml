				</packagingExcludes>
				</configuration>
			</plugin>
			<plugin>
				<groupId>org.apache.maven.plugins</groupId>
				<artifactId>maven-surefire-plugin</artifactId>
				<version>2.19.1</version>
				<configuration>
					<redirectTestOutputToFile>true</redirectTestOutputToFile>
					<!-- Java 17 Module System Compatibility -->
					<argLine>
						--add-opens=java.base/java.util=ALL-UNNAMED
						--add-opens=java.base/java.lang=ALL-UNNAMED
						--add-opens=java.base/java.io=ALL-UNNAMED
						--add-opens=java.base/java.net=ALL-UNNAMED
						--add-opens=java.base/java.time=ALL-UNNAMED
					</argLine>
					<!-- Fork configuration for better test isolation -->
					<!-- CRITICAL FIX: reuseForks=true to avoid "Error creating properties files for forking" -->
					<forkCount>1</forkCount>
					<reuseForks>true</reuseForks>
					<!-- Extended to 3 hours for full TCK suite execution -->
					<forkedProcessTimeoutInSeconds>10800</forkedProcessTimeoutInSeconds>
					<!-- Individual test timeout -->
					<timeout>600</timeout>
					<!-- System properties for test execution -->
					<systemPropertyVariables>
						<test.timeout>600000</test.timeout>
					</systemPropertyVariables>
				</configuration>
			</plugin>

			<plugin>
				<groupId>org.eclipse.jetty</groupId>
				<artifactId>jetty-maven-plugin</artifactId>
				<version>11.0.24</version>
