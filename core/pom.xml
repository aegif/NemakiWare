						<id>copy-jakarta-converted</id>
						<phase>prepare-package</phase>
						<goals>
							<goal>run</goal>
						</goals>
						<configuration>
							<target>
								<!-- Remove ALL conflicting libraries using wildcards -->
								<delete failonerror="false">
									<fileset dir="${project.build.directory}/${project.build.finalName}/WEB-INF/lib">
										<!-- Remove javax JAX-WS libraries -->
										<include name="jaxws-api-*.jar"/>
										<include name="jaxws-eclipselink-plugin-*.jar"/>
										<include name="jaxws-tools-*.jar"/>
										<include name="jaxws-ri-*.jar"/>
										<include name="jaxws-rt-4.0.0.jar"/>
										<include name="webservices-rt-*.jar"/>
										<include name="javax.xml.ws-*.jar"/>
