							<name>solr.host</name>
							<value>localhost</value>
						</systemProperty>
						<systemProperty>
							<name>solr.port</name>
							<value>8983</value>
						</systemProperty>
						<systemProperty>
							<name>solr.protocol</name>
							<value>http</value>
						</systemProperty>
						<systemProperty>
							<name>solr.context</name>
							<value>solr</value>
						</systemProperty>
